  <div class="transitions-enabled" id="pictures">
    <% @pictures.each do |picture| %>
    <div class="box panel panel-default">
      <img src=<%="#{picture.image}"%>>
      <div class="panel-body">
        <h2>
          <%= picture.user.display_name %>
        </h2>
      </div>
    </div>
    <% end %>
  </div>
